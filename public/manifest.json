{
  "manifest_version": 2,
  "name": "Microsoft Stream-shortcuts",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "64": "icons/icon_64.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://web.microsoftstream.com/video/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}